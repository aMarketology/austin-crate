# Environment Variables Template
# Copy this file to .env.local and fill in your actual values

# Google Analytics 4 Measurement ID
# Get from: https://analytics.google.com
# Format: G-XXXXXXXXXX
NEXT_PUBLIC_GA_ID=G-8G3X2DX8L2

# Mailjet API Credentials (for contact form)
# Get from: https://app.mailjet.com/account/apikeys
MAILJET_API_KEY=
MAILJET_SECRET_KEY=

# Contact form notification emails
NOTIFICATION_EMAIL_1=hello@austincrate.com
NOTIFICATION_EMAIL_2=info@amarketology.com

# Google Site Verification Code
# Get from: https://search.google.com/search-console
GOOGLE_SITE_VERIFICATION=

# Facebook Pixel ID (optional)
# Get from: https://business.facebook.com
NEXT_PUBLIC_FB_PIXEL_ID=

# Production URL
NEXT_PUBLIC_SITE_URL=https://austincrate.com
